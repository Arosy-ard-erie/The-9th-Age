    <profile id="ce82-839f-9c07-86df" profileTypeId="576561706f6e23232344415441232323" name="Lance" hidden="false">
      <characteristics>
        <characteristic characteristicId="52616e676523232344415441232323" name="Range"/>
        <characteristic characteristicId="537472656e67746823232344415441232323" name="Strength" value=""/>
