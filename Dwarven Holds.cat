          <modifiers/>
          <rules>
            <rule id="3736-9b2f-750d-a255" name="Rune of Excellence" hidden="false">
              <description>​Weapon&apos;s type changes to Great Weapon
</description>
              <modifiers/>
            </rule>
          </rules>
          <profiles/>
          <links/>
        </entry>
        <entry id="5b6a-37b1-29c8-4074" name="Rune of Haste" points="20.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="3" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false">
          <entries/>
          <entryGroups/>
          <modifiers/>
          <rules>
            <rule id="ea02-8a50-a547-3a90" name="Rune of Haste" hidden="false">
              <description>​For each of these runes Attacks are made at +2 to initiative</description>
              <modifiers/>
            </rule>
          </rules>
          <profiles/>
          <links/>
        </entry>
        <entry id="23f3-bbbb-01c2-b3a1" name="Rune of Flint" points="5.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false">
          <entries/>
          <entryGroups/>
          <modifiers/>
          <rules>
            <rule id="9f15-2eae-0a47-9ef8" name="Rune of Flint" hidden="false">
              <description>​Attacks gain Flaming attacks special rule.</description>
              <modifiers/>
            </rule>
          </rules>
          <profiles/>
          <links/>
        </entry>
        <entry id="ff18-0c4c-ac8e-b2dd" name="Rune of Penetrating" points="40.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false">
          <entries/>
