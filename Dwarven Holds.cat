          <rules>
            <rule id="b426-2bfb-9d70-c0df" name="Rune of Penetrating" hidden="false">
              <description>​Attacks by the weapon with this rune are made with AP(6)
</description>
              <modifiers/>
            </rule>
          </rules>
          <profiles/>
          <links/>
        </entry>
        <entry id="62a0-eb19-b170-69f5" name="Rune of Smashing" points="40.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false">
          <entries/>
